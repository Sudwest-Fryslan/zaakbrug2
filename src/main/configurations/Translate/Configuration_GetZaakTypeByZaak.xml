<Module xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../FrankConfig.xsd">
    <Adapter name="GetZaakTypeByZaak"
        active="${GetZaakTypeByZaak.Active}"
        description="">

        <Receiver name="GetZaakTypeByZaak">
            <JavaListener name="GetZaakTypeByZaak"/>
        </Receiver>

        <Pipeline>
            <Exits>
                <Exit path="EXIT" state="SUCCESS"/>
                <Exit path="EXCEPTION" state="ERROR"/>
            </Exits>

			<SenderPipe name="GetZgwZaakTypeByZaak" 
                getInputFromFixedValue="&lt;dummy/&gt;">
				<HttpSender 
					name="GetZgwZaakTypeByZaakSender" 
					methodType="GET"
					headersParams="Authorization,Accept-Crs"
					url="$ZaakUrl"
                    timeout="${creeerZaak.timeout}"
				/>
                <Param name="ZaakUrl" sessionKey="ZaakUrl"/>
                <Param name="Accept-Crs" value="EPSG:4326"/>
                <Param name="Authorization" value="Bearer ${JwtToken}"/>
                <Forward name="success" path="JsonToXml" />
			</SenderPipe>

            <JsonPipe name="JsonToXml"
                storeResultInSessionKey="GetZaakTypeResult">
                <Forward name="success" path="EXIT"/>
            </JsonPipe>
        </Pipeline>
    </Adapter>
</Module>